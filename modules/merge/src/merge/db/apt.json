{
	"name": "apt",
	"mir_pm_pairs": {
		"install <package_list>": "install <package_list>",
		"update <package_list>": "-U <package_list>",
		"search <package_list>": "-Q <package_list>",
		"remove <package_list>": "-R <package_list>",
		"--upgrade-packages": "upgrade",
		"install <package_list> --upgrade-packages": "-Syu <package_list>"
	}
}
